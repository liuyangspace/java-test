<?xml version="1.0" encoding="UTF-8"?>
<!-- RawPath : WEB-INFO/web.xml -->
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <!-- listener -->

    <listener>
        <listener-class>test.javaee.servlet.listener.RequestListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>RequestListener</servlet-name>
        <servlet-class>test.javaee.servlet.listener.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RequestListener</servlet-name>
        <url-pattern>/listener</url-pattern>
    </servlet-mapping>

</web-app>
